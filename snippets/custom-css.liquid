<style>
.visually-hidden {
    position: absolute;
    width: 1px;
    height: 1px;
    margin: -1px;
    padding: 0;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    border: 0;
}

.swatch.Color  {
    opacity: 0 !important;
}

.template-product .swatch .swatch-element>label:hover,
.template-product  .swatch .swatch-element input:checked+label {
    color: #fff;
    background-color: #292929;
    border-color: #292929;
    text-decoration-color: #fff;
}

.template-product  .swatch .swatch-element>label{
    padding:9px 10px;
    
}

.template-product .swatch  .header {
    display: block !important;
    width: 100%;
    font-weight: 400;
}

.jdgm-preview-badge .jdgm-star {
    color: #c01444;
}

@media(min-width:1200px){
.template-product .ltn__product-details-menu-1:has(.swatch) {
    aspect-ratio: 545/112;
}
.template-product .swatch-element.soldout label:before,
.swatch-element.soldout label:after {
    top: 0;
    background: #323232;
}

.template-product  .swatch-element.soldout label:before {
    opacity: 0;
}

.template-product .swatch .swatch-element>label {
    line-height: 16.6px;
}


  
  
.template-index  .ltn__feature-area  .ltn__feature-item-box-wrap.ltn__feature-item-box-wrap-2.ltn__border.section-bg-6 > .ltn__feature-item.ltn__feature-item-8 {
    width: 37%;
}

.template-product .ltn__shop-details-img-gallery .ltn__shop-details-large-img:not(.slick-initialized) .single-large-img:nth-child(n+2) {
    display: none;
}

.template-product  .ltn__shop-details-img-gallery  .ltn__shop-details-small-img:not(.slick-initialized) .single-small-img:nth-child(n+5) {
    display: none;
}

.template-product .ltn__shop-details-img-gallery  .ltn__shop-details-small-img:not(.slick-initialized) {
    display: flex;
}

.template-product .ltn__shop-details-large-img:not(.slick-initialized) {
    aspect-ratio: 570/759;
}

html:not(.w3_start ) .template-product .site-logo img {
    height: 99px;
}  
}

@media(max-width:767px){
.template-product .ltn__shop-details-img-gallery .ltn__shop-details-large-img:not(.slick-initialized) .single-large-img:nth-child(n+2) {
    display: none;
}

.template-product  .ltn__shop-details-img-gallery  .ltn__shop-details-small-img:not(.slick-initialized) .single-small-img:nth-child(n+4) {
    display: none;
}

.template-product .ltn__shop-details-img-gallery  .ltn__shop-details-small-img:not(.slick-initialized) {
    display: flex;
}

html:not(.w3_start) .template-product .ltn__shop-details-img-gallery {
    aspect-ratio: 345/633;
}

html:not(.w3_start) .template-product  .logo img {
    height: 49px;
}

.template-product .ltn__shop-details-large-img:not(.slick-initialized) {
    aspect-ratio: 345/459;
}

.template-product .ltn__shop-details-small-img:not(.slick-initialized) {
    aspect-ratio: 355/154;
}  
}

